{
  "name": "omnifex-ui",
  "version": "0.0.0",
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "nx serve omnifex-ui",
    "build": "nx build omnifex-ui",
    "build:identity": "nx build identity",
    "build:styles": "nx build styles",
    "build:ui-components": "cd libs/ui-components && ../../node_modules/.bin/pnpm run build",
    "watch": "nx build omnifex-ui --watch --configuration development",
