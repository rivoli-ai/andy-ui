    "version": "changeset version",
    "release": "changeset publish"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
