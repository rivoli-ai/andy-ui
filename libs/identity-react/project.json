{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@omnifex/identity-react",
  "projectType": "library",
  "sourceRoot": "libs/identity-react/src",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["@omnifex/identity:build"],
      "options": {
        "outputPath": "dist/identity-react",
        "main": "libs/identity-react/index.ts",
        "tsConfig": "libs/identity-react/tsconfig.lib.prod.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/identity-react/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/identity-react/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/identity-react/**/*.{ts,tsx}"],
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/identity-react"],
      "options": {
        "jestConfig": "libs/identity-react/jest.config.cjs",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:identity", "framework:react"],
  "implicitDependencies": ["@omnifex/identity"]
}

